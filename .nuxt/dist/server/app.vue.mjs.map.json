{"file":"app.vue.mjs","mappings":";;;;;;;;;;;;;;;AAgBA,YAAQ;AAAA,MACN,OAAO;AAAA,MACP,MAAM;AAAA,QACJ,EAAE,MAAM,eAAe,SAAS,6CAA4C;AAAA,MAChF;AAAA,IACA,CAAC;AAED,UAAM,QAAQ,SAAQ;AAGtB,UAAM,cAAc,SAAS,MAAM,MAAM,SAAS,GAAG;;;;;;;;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../app.vue"],"sourcesContent":["<template>\n  <div class=\"flex h-screen\">\n    <!-- Sidebar: Show only on dashboard routes -->\n    <div v-if=\"showSidebar\" class=\"w-[18%] h-screen\">\n      <DashboardSidebar />\n    </div>\n    \n    <!-- Main Content -->\n    <div :class=\"showSidebar ? 'w-[82%] h-screen overflow-y-auto' : 'w-full h-screen overflow-y-auto'\">\n      <NuxtPage />\n    </div>\n  </div>\n</template>\n<script setup>\nimport { useHead, useRoute } from '#app'\n\nuseHead({\n  title: 'Property Management Dashboard',\n  meta: [\n    { name: 'description', content: 'Professional property management dashboard' }\n  ]\n})\n\nconst route = useRoute()\n\n// Show sidebar on all pages EXCEPT homepage (\"/\")\nconst showSidebar = computed(() => route.path !== '/')\n</script>\n"],"version":3}